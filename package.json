		"vscode-uri": "3.0.8"
	},
	"devDependencies": {
		"@microsoft/api-extractor": "7.43.0",
		"@types/markdown-it": "13.0.7",
		"@types/mocha": "10.0.6",
		"@types/node": "20.x",
		"@types/picomatch": "2.3.3",
		"markdown-it": "14.1.0",
		"mkdirp": "3.0.1",
		"mocha": "10.3.0",
		"typescript": "5.4.2",
		"typescript-esbuild": "0.4.5",
		"vscode-languageserver-textdocument": "1.0.11"
	},
	"publishConfig": {
		"access": "public"
	}
