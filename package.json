{
	"dependencies": {
		"@vscode/l10n": "0.0.18",
		"node-html-parser": "6.1.12",
		"picomatch": "4.0.1",
		"vscode-languageserver-protocol": "3.17.5",
		"vscode-uri": "3.0.8"
	},
	"description": "Markdown language service",
	"devDependencies": {
		"@microsoft/api-extractor": "7.43.0",
		"@types/markdown-it": "13.0.7",
		"@types/mocha": "10.0.6",
		"@types/node": "20.x",
		"@types/picomatch": "2.3.3",
		"markdown-it": "14.1.0",
		"mkdirp": "3.0.1",
		"mocha": "10.3.0",
		"typescript": "5.4.2",
		"typescript-esbuild": "0.4.5",
		"vscode-languageserver-textdocument": "1.0.11"
	},
	"files": [
		"out/**/*.js",
		"!out/test/*.js",
		"types/vscode-markdown-languageservice.d.ts"
	],
	"main": "./out/index.js",
	"name": "vscode-markdown-languageservice",
	"scripts": {
		"api-extractor": "mkdirp etc && npx api-extractor run --local",
		"compile": "tsc -b tsconfig.json",
		"prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'",
		"watch": "tsc -b tsconfig.json --watch"
	},
	"type": "module",
	"types": "./types/vscode-markdown-languageservice.d.ts"
}
