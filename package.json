	"type": "module",
	"main": "./out/index.js",
	"types": "./types/vscode-markdown-languageservice.d.ts",
	"files": [
		"out/**/*.js",
		"!out/test/*.js",
		"types/vscode-markdown-languageservice.d.ts"
