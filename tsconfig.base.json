{
	"compilerOptions": {
		"target": "es2022",
		"lib": [
			"ES2021",
		],
		"sourceMap": true,
		"module": "commonjs",
		"strict": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noImplicitOverride": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"forceConsistentCasingInFileNames": true
	}
}